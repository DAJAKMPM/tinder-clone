{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@app/components/*": ["src/components/*"],
      "@app/hooks": ["src/hooks"],
      "@app/navigators": ["src/navigators"],
      "@app/screens/*": ["src/screens/*"],
      "@app/providers/*": ["src/providers/*"],
      "@app/config/*": ["src/config/*"]
    }
  },
  "typeRoots": ["./src/types"]
}
